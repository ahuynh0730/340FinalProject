<h1> <a href = "/"> TKO Restaurant </a> </h1>

<h3> Employees Page </h3>

<table>
	<tr>
		<th> ID </th>
		<th> First Name </th>
		<th> Last Name </th>
	</tr>
	
	

	{{#if results}}
		{{#each results}}
			<tr>
				<td> {{this.id}} </td>
				<td> {{this.fname}} </td>
				<td> {{this.lname}} </td>
			</tr>
		{{/each}}
    {{/if}}
	
</table>